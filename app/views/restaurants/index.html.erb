<h2>C'EST MA BITE</h2>
<h2> All restaurants </h2>
<ul class="list-unstyled">
  <% @restaurants.each do |restaurant| %>
    <li><%= link_to restaurant.name, restaurant_path(restaurant) %></li>
  <% end %>
</ul>

<i class="fa fa-plus">
  <%= link_to "Add a new restaurant", new_restaurant_path, class: "btn btn-primary" %>
</i>
